{
    "name": "cowegis/cowegis-client-bundle",
    "type": "symfony-bundle",
    "description": "Cowegis Client - This bundle provides the cowegis javascript client as assets for symfony applications",
    "keywords": [
        "cowegis",
        "leaflet",
        "webgis",
        "gis",
        "javascript"
    ],
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "David Molineus",
            "email": "david.molineus@netzmacht.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/http-kernel": "^4.4"
    },
    "require-dev": {
        "doctrine/coding-standard": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Cowegis\\Bundle\\Client\\": "src/"
        }
    }
}
